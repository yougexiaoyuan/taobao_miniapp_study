    line-height: 40px;
    margin-top: 10px;
    box-shadow: grey;
    border-radius: 5px;
    border: 1px solid #ddd;
    transition: all .2s ease-in-out;
    cursor: pointer;
}
.rule_select:hover {
  transform: scale(1.1);
  border-left: 5px solid #66AAFF;
  border-right: 5px solid #66AAFF;
