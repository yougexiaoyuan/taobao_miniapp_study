.ivu-row {
    position: relative;
margin-left: -4px;
    margin-right: -4px;
    height: auto;
    zoom: 1;
    display: block;
}
.ivu-row:after, .ivu-row:before {
    content: "";
    display: table;
}
.ivu-row:after {
    clear: both;
    visibility: hidden;
    font-size: 0;
    height: 0;
}
.ivu-card-bordered {
