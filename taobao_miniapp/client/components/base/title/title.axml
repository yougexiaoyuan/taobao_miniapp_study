<view>
  <view data-url="{{linkConfig}}" class="title" onTap="clickEvent" style="overflow: scroll;word-break: break-all;position:absolute;width:{{width}}rpx;height:{{height}}rpx;top:{{top}}rpx;left:{{left}}rpx;z-index:{{zindex}};background-image:url({{logoConfig}});background-size:100% 100%;background-repeat:no-repeat;">
    <view class="{{textStyle==1?'italics':textStyle==2?'blod':''}} {{textPosition==0?'left':textPosition==2?'right':''}}" style="font-size:{{fontSize}}rpx;color:{{titleColor}};margin-top:{{mbConfig}}rpx;"><rich-text nodes="{{titleConfig}}"></rich-text></view>
  </view>
</view>