.container{
    background:none;
}
.card{
    width:90%;
    margin:0 auto;
    border-radius: 10rpx;
    background:white;
    margin-top:20rpx;
    padding:20rpx;
}
.card .card-title{
    font-weight: bold;
    font-size:15px;
    line-height:100rpx;
    border-bottom:1px solid #ccc;
}
.card .card-content{
    font-size:12px;
    color:#999;
    margin-top:30rpx;
    line-height:50rpx;
}